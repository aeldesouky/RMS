@page
@model RMS.Pages.ToItCreateProfTA

@{
    Layout = "~/Pages/Shared/_ToIt.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <title></title>
</head>
<body>
<h1>Add Prof</h1>
<br/>
<form method="post">
    <div class="form-group">
        <label for="Name">Name:</label>
        <input type="text" class="form-control" placeholder="Enter Name" id="Name" asp-for="@Model.Name" required/>
    </div>
    

    <div class="form-group">
        <label for="ID">ID:</label>
        <input type="number" class="form-control" placeholder="Enter ID" id="ID" asp-for="@Model.ID" required/>
    </div>

    
    <div class="form-group">
        <label for="pwd">Password:</label>
        <input type="password" class="form-control" placeholder="Enter password" id="pwd" asp-for="@Model.Password" required/>
    </div>

    <button type="submit" class="btn btn-primary" style="margin: 10px">Add Prof</button>
</form>
</body>
</html>